{
  "name": "UW Time Schedule",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Makeover of the University of Washington Time Schedule",
  "browser_action": {
    "name": "Manipulate DOM",
    "icon": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "css": ["time_schedule.css"],
    "js": ["jquery-1.8.3.min.js", "background.js"],
    "matches": ["http://www.washington.edu/students/timeschd/*/"]
  }],
  "permissions": [
    "http://api.flickr.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
